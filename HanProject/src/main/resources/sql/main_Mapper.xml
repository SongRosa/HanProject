<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="main">

 	<select id="select" resultType="spring.command.MainNoticeCommand">
		select n_number, n_subject, n_regdate
		from notice order by n_number desc
	</select>

</mapper>