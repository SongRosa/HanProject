<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="facility">
    
    <select id="selectList" resultType="spring.command.FacilityCommand" parameterType="int">
        select * from location where p_number = #{p_number} or f_number = 5      
    </select>
    
</mapper>